{
    "name": "seankndy/react-php-alertmanager",
    "description": "async (reactphp) alert routing/dispatching",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "react/http": "^1",
        "react/child-process": "^0.6.1",
        "swiftmailer/swiftmailer": "^6.2",
        "ramsey/uuid": "^4.0",
        "nikic/fast-route": "^1.3",
        "nesbot/carbon": "^2.54",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {"SeanKndy\\AlertManager\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sean Kennedy",
            "email": "sean@kndy.net"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spatie/test-time": "^1.2",
        "rector/rector": "^0.12.4"
    }
}
