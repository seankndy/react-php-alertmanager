{
    "name": "seankndy/react-php-alertmanager",
    "description": "async (reactphp) alert routing/dispatching",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "react/http": "^0.8.4",
        "react/child-process": "^0.6.1",
        "swiftmailer/swiftmailer": "^6.2",
        "ramsey/uuid": "^4.0",
        "nikic/fast-route": "^1.3",
        "react/http-client": "^0.5.9"
    },
    "autoload": {
        "psr-4": {"SeanKndy\\AlertManager\\": "src/"}
